{
  "expo": {
    "name": "스펙토",
    "slug": "specto-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/184cbd4a-4986-4e07-91df-ab93a9285f32?runtime-version=1.0.0&channel-name=production&platform=android",
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spectacle.specto"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.spectacle.specto",
      "versionCode": 1,
      "permissions": ["NOTIFICATIONS"],
      "useNextNotificationsApi": true,
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "184cbd4a-4986-4e07-91df-ab93a9285f32"
      }
    },
    "owner": "bongjs",
    "plugins": ["expo-font", "expo-secure-store", "expo-asset"]
  }
}
